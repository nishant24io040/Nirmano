<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/frameLayout"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".homefrag">


    <ImageView
        android:id="@+id/blurbg"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:scaleType="centerCrop"
        android:src="@drawable/landingblur"
        android:visibility="invisible" />

    <ScrollView
        android:id="@+id/scrollView2"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="180dp"
        android:paddingBottom="20dp"
        android:visibility="visible">

        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content">

            <androidx.cardview.widget.CardView
                android:id="@+id/cardView19"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center_horizontal"
                app:cardCornerRadius="25dp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="vertical">

                    <androidx.constraintlayout.widget.ConstraintLayout
                        android:id="@+id/linearLayout"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center"
                        android:background="@drawable/homemidbg">

                        <ImageView
                            android:id="@+id/imageView3"
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:scaleType="fitXY"
                            android:src="@drawable/midcurve"
                            app:layout_constraintEnd_toEndOf="parent"
                            app:layout_constraintHorizontal_bias="0.0"
                            app:layout_constraintStart_toStartOf="parent"
                            app:layout_constraintTop_toTopOf="parent" />

                        <TextView
                            android:id="@+id/textView4"
                            android:layout_width="313dp"
                            android:layout_height="48dp"
                            android:layout_marginTop="14dp"
                            android:fontFamily="@font/poppins_medium"
                            android:gravity="center"
                            android:text="How do you feel right now?"
                            android:textColor="#000000"
                            android:textSize="16dp"
                            app:layout_constraintEnd_toEndOf="@+id/imageView3"
                            app:layout_constraintHorizontal_bias="0.0"
                            app:layout_constraintStart_toStartOf="@+id/imageView3"
                            app:layout_constraintTop_toTopOf="@+id/imageView3" />

                        <androidx.cardview.widget.CardView
                            android:id="@+id/surprised"
                            android:layout_width="141dp"
                            android:layout_height="141dp"
                            android:background="@drawable/lovedbg"
                            app:cardCornerRadius="35dp"
                            app:layout_constraintBottom_toBottomOf="@+id/surprised"
                            app:layout_constraintEnd_toEndOf="@+id/cardView4"
                            app:layout_constraintStart_toStartOf="@+id/cardView4"
                            app:layout_constraintTop_toTopOf="@+id/surprised">

                            <androidx.constraintlayout.widget.ConstraintLayout
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:background="@drawable/surprisedbg">

                                <TextView
                                    android:id="@+id/textView3"
                                    android:layout_width="109dp"
                                    android:layout_height="32dp"
                                    android:fontFamily="@font/poppins_medium"
                                    android:gravity="center_vertical"
                                    android:paddingStart="16dp"
                                    android:text="Surprised"
                                    android:textColor="#000000"
                                    android:textSize="16dp"
                                    app:layout_constraintBottom_toBottomOf="@+id/imageView5"
                                    app:layout_constraintEnd_toEndOf="@+id/imageView5"
                                    app:layout_constraintHorizontal_bias="0.161"
                                    app:layout_constraintStart_toStartOf="parent"
                                    app:layout_constraintTop_toTopOf="parent"
                                    app:layout_constraintVertical_bias="0.146" />

                                <ImageView
                                    android:id="@+id/imageView5"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_gravity="bottom|end"
                                    android:src="@drawable/surprised"
                                    app:layout_constraintBottom_toBottomOf="parent"
                                    app:layout_constraintEnd_toEndOf="parent"
                                    app:layout_constraintHorizontal_bias="0.862"
                                    app:layout_constraintStart_toStartOf="parent" />
                            </androidx.constraintlayout.widget.ConstraintLayout>

                        </androidx.cardview.widget.CardView>

                        <androidx.cardview.widget.CardView
                            android:id="@+id/surprised"
                            android:layout_width="141dp"
                            android:layout_height="141dp"
                            android:layout_marginTop="20dp"
                            android:layout_marginBottom="23dp"
                            android:background="@drawable/lovedbg"
                            app:cardCornerRadius="35dp"
                            app:layout_constraintBottom_toBottomOf="parent"
                            app:layout_constraintEnd_toEndOf="@+id/loved"
                            app:layout_constraintStart_toStartOf="@+id/loved"
                            app:layout_constraintTop_toBottomOf="@+id/cardView3">

                            <androidx.constraintlayout.widget.ConstraintLayout
                                android:id="@+id/feared"
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:background="@drawable/fearedbg">

                                <TextView
                                    android:id="@+id/textView3"
                                    android:layout_width="109dp"
                                    android:layout_height="32dp"
                                    android:fontFamily="@font/poppins_medium"
                                    android:gravity="center_vertical"
                                    android:paddingStart="16dp"
                                    android:text="Feared"
                                    android:textColor="#000000"
                                    android:textSize="16dp"
                                    app:layout_constraintBottom_toBottomOf="@+id/imageView5"
                                    app:layout_constraintEnd_toEndOf="@+id/imageView5"
                                    app:layout_constraintHorizontal_bias="0.161"
                                    app:layout_constraintStart_toStartOf="parent"
                                    app:layout_constraintTop_toTopOf="parent"
                                    app:layout_constraintVertical_bias="0.146" />

                                <ImageView
                                    android:id="@+id/imageView5"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_gravity="bottom|end"
                                    android:src="@drawable/feared"
                                    app:layout_constraintBottom_toBottomOf="parent"
                                    app:layout_constraintEnd_toEndOf="parent"
                                    app:layout_constraintHorizontal_bias="0.862"
                                    app:layout_constraintStart_toStartOf="parent" />
                            </androidx.constraintlayout.widget.ConstraintLayout>

                        </androidx.cardview.widget.CardView>

                        <androidx.cardview.widget.CardView
                            android:id="@+id/cardView4"
                            android:layout_width="141dp"
                            android:layout_height="141dp"
                            android:layout_marginTop="20dp"
                            android:background="@drawable/lovedbg"
                            app:cardCornerRadius="35dp"
                            app:layout_constraintEnd_toEndOf="@+id/joyful"
                            app:layout_constraintStart_toStartOf="@+id/joyful"
                            app:layout_constraintTop_toBottomOf="@+id/joyful">

                            <androidx.constraintlayout.widget.ConstraintLayout
                                android:id="@+id/sadcons"
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:background="@drawable/sadbg">

                                <TextView
                                    android:id="@+id/textView3"
                                    android:layout_width="109dp"
                                    android:layout_height="32dp"
                                    android:fontFamily="@font/poppins_medium"
                                    android:gravity="center_vertical"
                                    android:paddingStart="16dp"
                                    android:text="Sad"
                                    android:textColor="#000000"
                                    android:textSize="16dp"
                                    app:layout_constraintBottom_toBottomOf="@+id/imageView5"
                                    app:layout_constraintEnd_toEndOf="@+id/imageView5"
                                    app:layout_constraintHorizontal_bias="0.161"
                                    app:layout_constraintStart_toStartOf="parent"
                                    app:layout_constraintTop_toTopOf="parent"
                                    app:layout_constraintVertical_bias="0.146" />

                                <ImageView
                                    android:id="@+id/imageView5"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_gravity="bottom|end"
                                    android:src="@drawable/sad"
                                    app:layout_constraintBottom_toBottomOf="parent"
                                    app:layout_constraintEnd_toEndOf="parent"
                                    app:layout_constraintHorizontal_bias="0.862"
                                    app:layout_constraintStart_toStartOf="parent" />
                            </androidx.constraintlayout.widget.ConstraintLayout>

                        </androidx.cardview.widget.CardView>

                        <androidx.cardview.widget.CardView
                            android:id="@+id/cardView3"
                            android:layout_width="141dp"
                            android:layout_height="141dp"
                            android:layout_marginTop="20dp"
                            android:background="@drawable/lovedbg"
                            app:cardCornerRadius="35dp"
                            app:layout_constraintEnd_toEndOf="@+id/loved"
                            app:layout_constraintStart_toStartOf="@+id/loved"
                            app:layout_constraintTop_toBottomOf="@+id/loved">

                            <androidx.constraintlayout.widget.ConstraintLayout
                                android:id="@+id/angry"
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:background="@drawable/angrybg">

                                <TextView
                                    android:id="@+id/textView3"
                                    android:layout_width="109dp"
                                    android:layout_height="32dp"
                                    android:fontFamily="@font/poppins_medium"
                                    android:gravity="center_vertical"
                                    android:paddingStart="16dp"
                                    android:text="Angry"
                                    android:textColor="#000000"
                                    android:textSize="16dp"
                                    app:layout_constraintBottom_toBottomOf="@+id/imageView5"
                                    app:layout_constraintEnd_toEndOf="@+id/imageView5"
                                    app:layout_constraintHorizontal_bias="0.161"
                                    app:layout_constraintStart_toStartOf="parent"
                                    app:layout_constraintTop_toTopOf="parent"
                                    app:layout_constraintVertical_bias="0.146" />

                                <ImageView
                                    android:id="@+id/imageView5"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_gravity="bottom|end"
                                    android:src="@drawable/angry"
                                    app:layout_constraintBottom_toBottomOf="parent"
                                    app:layout_constraintEnd_toEndOf="parent"
                                    app:layout_constraintHorizontal_bias="0.862"
                                    app:layout_constraintStart_toStartOf="parent" />
                            </androidx.constraintlayout.widget.ConstraintLayout>

                        </androidx.cardview.widget.CardView>

                        <androidx.cardview.widget.CardView
                            android:id="@+id/joyful"
                            android:layout_width="141dp"
                            android:layout_height="141dp"
                            android:layout_marginStart="20dp"
                            android:layout_marginEnd="26dp"
                            android:background="@drawable/lovedbg"
                            app:cardCornerRadius="35dp"
                            app:layout_constraintEnd_toEndOf="parent"
                            app:layout_constraintStart_toEndOf="@+id/loved"
                            app:layout_constraintTop_toTopOf="@+id/loved">

                            <androidx.constraintlayout.widget.ConstraintLayout
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:background="@drawable/joyfulbg">

                                <TextView
                                    android:id="@+id/textView3"
                                    android:layout_width="109dp"
                                    android:layout_height="32dp"
                                    android:fontFamily="@font/poppins_medium"
                                    android:gravity="center_vertical"
                                    android:paddingStart="16dp"
                                    android:text="Joyful"
                                    android:textColor="#000000"
                                    android:textSize="16dp"
                                    app:layout_constraintBottom_toBottomOf="@+id/imageView5"
                                    app:layout_constraintEnd_toEndOf="@+id/imageView5"
                                    app:layout_constraintHorizontal_bias="0.161"
                                    app:layout_constraintStart_toStartOf="parent"
                                    app:layout_constraintTop_toTopOf="parent"
                                    app:layout_constraintVertical_bias="0.146" />

                                <ImageView
                                    android:id="@+id/imageView5"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_gravity="bottom|end"
                                    android:src="@drawable/joyful"
                                    app:layout_constraintBottom_toBottomOf="parent"
                                    app:layout_constraintEnd_toEndOf="parent" />
                            </androidx.constraintlayout.widget.ConstraintLayout>

                        </androidx.cardview.widget.CardView>

                        <androidx.cardview.widget.CardView
                            android:id="@+id/loved"
                            android:layout_width="141dp"
                            android:layout_height="141dp"
                            android:layout_marginStart="26dp"
                            android:layout_marginTop="30dp"
                            android:background="@drawable/lovedbg"
                            app:cardCornerRadius="35dp"
                            app:layout_constraintEnd_toStartOf="@+id/joyful"
                            app:layout_constraintStart_toStartOf="parent"
                            app:layout_constraintTop_toBottomOf="@+id/textView4">

                            <androidx.constraintlayout.widget.ConstraintLayout
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:background="@drawable/lovedbg">

                                <TextView
                                    android:id="@+id/textView3"
                                    android:layout_width="109dp"
                                    android:layout_height="32dp"
                                    android:fontFamily="@font/poppins_medium"
                                    android:gravity="center_vertical"
                                    android:paddingStart="16dp"
                                    android:text="Loved"
                                    android:textColor="#000000"
                                    android:textSize="16dp"
                                    app:layout_constraintBottom_toBottomOf="@+id/imageView5"
                                    app:layout_constraintEnd_toEndOf="@+id/imageView5"
                                    app:layout_constraintHorizontal_bias="0.161"
                                    app:layout_constraintStart_toStartOf="parent"
                                    app:layout_constraintTop_toTopOf="parent"
                                    app:layout_constraintVertical_bias="0.146" />

                                <ImageView
                                    android:id="@+id/imageView5"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_gravity="bottom|end"
                                    android:src="@drawable/loved"
                                    app:layout_constraintBottom_toBottomOf="parent"
                                    app:layout_constraintEnd_toEndOf="parent"
                                    app:layout_constraintHorizontal_bias="0.862"
                                    app:layout_constraintStart_toStartOf="parent" />
                            </androidx.constraintlayout.widget.ConstraintLayout>

                        </androidx.cardview.widget.CardView>

                    </androidx.constraintlayout.widget.ConstraintLayout>

                </LinearLayout>
            </androidx.cardview.widget.CardView>

            <androidx.cardview.widget.CardView
                android:id="@+id/cardView20"
                android:layout_width="0dp"
                android:layout_height="163dp"
                android:layout_marginTop="36dp"
                android:layout_marginBottom="20dp"
                app:cardCornerRadius="25dp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="@+id/cardView19"
                app:layout_constraintStart_toStartOf="@+id/cardView19"
                app:layout_constraintTop_toBottomOf="@+id/cardView19">

                <androidx.constraintlayout.widget.ConstraintLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent">

                    <ImageView
                        android:id="@+id/imageView56"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:scaleType="fitXY"
                        android:src="@drawable/hobbycard"
                        app:layout_constraintBottom_toBottomOf="parent"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toTopOf="parent" />

                    <Button
                        android:id="@+id/button4"
                        android:layout_width="118dp"
                        android:layout_height="27dp"
                        android:layout_marginTop="8dp"
                        android:background="@drawable/button_view_profile"
                        android:backgroundTint="#FF9C27"
                        android:fontFamily="@font/poppins"
                        android:gravity="center"
                        android:text="Explore"
                        android:textAlignment="center"
                        android:textAllCaps="false"
                        android:textColor="@color/white"
                        android:textSize="18dp"
                        app:layout_constraintStart_toStartOf="@+id/textView43"
                        app:layout_constraintTop_toBottomOf="@+id/textView43" />

                    <TextView
                        android:id="@+id/textView19"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="21dp"
                        android:layout_marginTop="15dp"
                        android:fontFamily="@font/poppins"
                        android:text="My Hobby"
                        android:textColor="#000000"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toTopOf="parent" />

                    <TextView
                        android:id="@+id/textView43"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="12dp"
                        android:layout_marginEnd="120dp"
                        android:fontFamily="@font/poppins"
                        android:text="Choose your favorite hobby and practice it daily building bits and bits of happiness"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintHorizontal_bias="0.0"
                        app:layout_constraintStart_toStartOf="@+id/textView19"
                        app:layout_constraintTop_toBottomOf="@+id/textView19" />

                    <ImageView
                        android:id="@+id/imageView57"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:src="@drawable/pottt"
                        app:layout_constraintBottom_toBottomOf="@+id/button4"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintTop_toTopOf="@+id/textView19" />
                </androidx.constraintlayout.widget.ConstraintLayout>
            </androidx.cardview.widget.CardView>
        </androidx.constraintlayout.widget.ConstraintLayout>
    </ScrollView>

</FrameLayout>
<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#FFFDEA"
    tools:context=".JoyActivity">

    <ImageView
        android:id="@+id/imageView19"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:scaleType="fitXY"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@drawable/positivebg" />

    <androidx.cardview.widget.CardView
        android:id="@+id/happy"
        android:layout_width="141dp"
        android:layout_height="141dp"
        android:background="@drawable/lovedbg"
        app:cardCornerRadius="13dp"
        app:layout_constraintBottom_toBottomOf="@+id/cheerful"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/cheerful"
        app:layout_constraintTop_toTopOf="@+id/cheerful">

        <ImageView
            android:id="@+id/imageView5"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:layout_marginBottom="10dp"
            android:src="@drawable/happy" />

        <TextView
            android:id="@+id/textView21"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_gravity="bottom|center_horizontal"
            android:layout_marginBottom="13dp"
            android:fontFamily="@font/poppins_medium"
            android:paddingStart="12dp"
            android:text="Happy"
            android:textColor="#000000" />

    </androidx.cardview.widget.CardView>

    <androidx.cardview.widget.CardView
        android:id="@+id/cheerful"
        android:layout_width="141dp"
        android:layout_height="141dp"
        android:layout_marginTop="30dp"
        android:background="@drawable/lovedbg"
        app:cardCornerRadius="13dp"
        app:layout_constraintEnd_toStartOf="@+id/happy"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/optimistic">

        <ImageView
            android:id="@+id/imageView5"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:layout_marginBottom="10dp"
            android:src="@drawable/cheerful" />

        <TextView
            android:id="@+id/textView21"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_gravity="bottom|center_horizontal"
            android:layout_marginBottom="13dp"
            android:fontFamily="@font/poppins_medium"
            android:paddingStart="12dp"
            android:text="Cheerful"
            android:textColor="#000000" />

    </androidx.cardview.widget.CardView>

    <androidx.cardview.widget.CardView
        android:id="@+id/pround"
        android:layout_width="141dp"
        android:layout_height="141dp"
        android:background="@drawable/lovedbg"
        app:cardCornerRadius="13dp"
        app:layout_constraintBottom_toBottomOf="@+id/optimistic"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/optimistic"
        app:layout_constraintTop_toTopOf="@+id/optimistic">

        <ImageView
            android:id="@+id/imageView5"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:layout_marginBottom="10dp"
            android:src="@drawable/proud" />

        <TextView
            android:id="@+id/textView21"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_gravity="bottom|center_horizontal"
            android:layout_marginBottom="13dp"
            android:fontFamily="@font/poppins_medium"
            android:paddingStart="12dp"
            android:shadowColor="#000000"
            android:text="Proud"
            android:textColor="#000000" />

    </androidx.cardview.widget.CardView>

    <androidx.cardview.widget.CardView
        android:id="@+id/optimistic"
        android:layout_width="141dp"
        android:layout_height="141dp"
        android:layout_marginTop="30dp"
        android:background="@drawable/lovedbg"
        app:cardCornerRadius="13dp"
        app:layout_constraintEnd_toStartOf="@+id/pround"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/Chaeming">

        <ImageView
            android:id="@+id/imageView5"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:layout_marginBottom="10dp"
            android:src="@drawable/optimistic" />

        <TextView
            android:id="@+id/textView21"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_gravity="bottom|center_horizontal"
            android:layout_marginBottom="13dp"
            android:fontFamily="@font/poppins_medium"
            android:paddingStart="12dp"
            android:text="Optimistic"
            android:textColor="#000000" />

    </androidx.cardview.widget.CardView>

    <androidx.cardview.widget.CardView
        android:id="@+id/Enthu"
        android:layout_width="141dp"
        android:layout_height="141dp"
        android:background="@drawable/lovedbg"
        app:cardCornerRadius="13dp"
        app:layout_constraintBottom_toBottomOf="@+id/Chaeming"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/Chaeming"
        app:layout_constraintTop_toTopOf="@+id/Chaeming">

        <ImageView
            android:id="@+id/imageView5"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:layout_marginBottom="10dp"
            android:src="@drawable/enthusiastic" />

        <TextView
            android:id="@+id/textView21"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_gravity="bottom|center_horizontal"
            android:layout_marginBottom="13dp"
            android:fontFamily="@font/poppins_medium"
            android:paddingStart="12dp"
            android:shadowColor="#000000"
            android:text="Enthusiastic"
            android:textColor="#000000" />

    </androidx.cardview.widget.CardView>

    <TextView
        android:id="@+id/backd1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="24dp"
        android:drawableStart="@drawable/baseline_arrow_back_24"
        android:drawablePadding="20dp"
        android:fontFamily="@font/dancing_script"
        android:gravity="center"
        android:text="Choose your perfect mood."
        android:textColor="#000000"
        android:textSize="28sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.149"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <androidx.cardview.widget.CardView
        android:id="@+id/Chaeming"
        android:layout_width="141dp"
        android:layout_height="141dp"
        android:layout_marginTop="56dp"
        android:background="@drawable/lovedbg"
        app:cardCornerRadius="13dp"
        app:layout_constraintEnd_toStartOf="@+id/Enthu"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/backd1">

        <ImageView
            android:id="@+id/imageView5"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:layout_marginBottom="10dp"
            android:src="@drawable/charming" />

        <TextView
            android:id="@+id/textView21"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_gravity="bottom|center_horizontal"
            android:layout_marginBottom="13dp"
            android:fontFamily="@font/poppins_medium"
            android:text="Charming"
            android:textColor="#000000"
            android:paddingStart="12dp" />

    </androidx.cardview.widget.CardView>
</androidx.constraintlayout.widget.ConstraintLayout>
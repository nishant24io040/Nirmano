<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    tools:context=".ClickOnFeeling.ClickOnSad.DepthOfEmotionLevel1.Level2.Upsetfrag.Depressed">

    <!-- TODO: Update blank fragment layout -->
    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/frameLayout3"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        tools:context=".ClickOnFeeling.ClickOnSad.DepthOfEmotionLevel1.Level2.Upsetfrag.Sorrow">

        <TextView
            android:id="@+id/textView16"
            android:layout_width="274dp"
            android:layout_height="wrap_content"
            android:layout_marginTop="30dp"
            android:fontFamily="@font/poppins"
            android:gravity="center_vertical"
            android:text="COMMON SYMPTOMS"
            android:textColor="#000000"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/firstdiscription2" />

        <TextView
            android:id="@+id/secbody2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:fontFamily="@font/poppins"
            android:gravity="center_vertical"
            android:text="@string/GUILTY"
            android:textAlignment="viewStart"
            android:textColor="#000000"
            app:layout_constraintStart_toStartOf="@+id/firstdiscription2"
            app:layout_constraintTop_toBottomOf="@+id/textView16" />

        <TextView
            android:id="@+id/firstdiscription2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="8dp"
            android:fontFamily="@font/poppins"
            android:gravity="center_vertical"
            android:text="A self conscious emotion characterized by a painful appraisal of having done (or thought) something that is wrong and often by a readiness to take action designed to undo or mitigate this wrong"
            android:textColor="#000000"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/imageView41" />

        <ImageView
            android:id="@+id/imageView41"
            android:layout_width="80dp"
            android:layout_height="17dp"
            android:layout_marginStart="-2dp"
            android:layout_marginTop="5dp"
            android:src="@drawable/level3"
            app:layout_constraintStart_toStartOf="@+id/firstdiscription2"
            app:layout_constraintTop_toTopOf="parent" />
    </androidx.constraintlayout.widget.ConstraintLayout>

</FrameLayout>